type Query {
    getMapStores : StoreLocatorOutput @resolver(class: "Mage4\\StoreLocator\\Model\\Resolver\\StoresResolver") @doc(description:"Test Module Datapassing")
}

type StoreLocatorOutput {
    total_count : String
    items : [StoreItem]
}

type StoreItem {
    id : Int
    name : String
    country : String
    state : String
    city : String
    zip : Int
    street : String
    lat : Int
    lng : Int
    phone : String
    email : String
    image : String
}
